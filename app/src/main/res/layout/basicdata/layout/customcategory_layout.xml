<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/custom_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context=".basicdata.CustomList"
   >

    <include layout="@layout/customcategory_toobar" android:id="@+id/customcategory_toobar"/>

    <com.example.androiderp.custom.CustomSearch
        android:id="@+id/customcategory_layout_search"
        style="@style/editTextStyle"
       />

    <ListView
        android:layout_marginTop="90dp"
        android:id="@+id/customcategory_layout_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </ListView>

</RelativeLayout>

